  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:codegen": "npm run codegen -- --watch",
    "dev:pathpida": "pathpida --ignorePath .gitignore --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "type-check": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@graphql-codegen/typed-document-node": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@next/font": "13.1.6",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "framer-motion": "^9.0.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "next": "13.1.6",
    "next-auth": "^4.19.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^4.9.5",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/client-preset": "2.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@tsconfig/strictest": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "eslint-config-prettier": "^8.6.0",
    "graphql-codegen-typescript-validation-schema": "^0.7.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "prettier": "^2.8.3"
  }
